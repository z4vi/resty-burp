<assembly>
  <id>with-dep</id>
  <formats>
    <format>jar</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <dependencySets>
    <dependencySet>
      <unpack>true</unpack>
      <scope>runtime</scope>
      <useProjectArtifact>false</useProjectArtifact>
    </dependencySet>
  </dependencySets>
  <fileSets>
    <fileSet>
      <directory>${project.build.outputDirectory}</directory>
      <outputDirectory></outputDirectory>
    </fileSet>
  </fileSets>
  <files>
	<file>
		<source>blank.burp.state</source>
		<outputDirectory>/</outputDirectory>
	</file>
	<file>
		<source>burp.config</source>
		<outputDirectory>/</outputDirectory>
	</file>
	<file>
		<source>log4j.properties</source>
		<outputDirectory>/</outputDirectory>
	</file>
  </files>
</assembly>
